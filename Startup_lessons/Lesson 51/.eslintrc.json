{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "react",
      "node"
    ],
    "ignorePatterns": [
      "webpack.config.js"
    ],
    "rules": {
      "no-unused-vars": "error",
      "react/prop-types": 0,
      "no-console": "error",
      "quotes": ["error", "single"],
      "jsx-quotes": ["error", "prefer-double"],
      "prefer-const": "error",
      "indent": ["warn", 2],
      "comma-dangle": ["error", "always-multiline"],
      "semi": ["error", "always"],
      "node/no-unsupported-features/es-syntax": "off"
    },
    "settings": {
      "react": {
        "version": "1.0.0"
      }
    }
  }
  